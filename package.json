{
	"name": "jroberts",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.1.7",
		"@types/node": "^25.2.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.32.1",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.7",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"axios": "^1.9.0",
		"daisyui": "^5.0.35",
		"moment": "^2.30.1",
		"uuid": "^11.1.0"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.{json,css,md,html}": [
			"prettier --write"
		]
	}
}
